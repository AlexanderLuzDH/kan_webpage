{
  "version": 2,
  "headers": [
    {
      "source": "/assets/img/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    { "source": "/blank-3", "destination": "/#contact", "permanent": true },
    { "source": "/blank-:slug", "destination": "/", "permanent": true },
    { "source": "/blank/:path*", "destination": "/", "permanent": true },
    { "source": "/contato", "destination": "/#contact", "permanent": true },
    { "source": "/contacto", "destination": "/#contact", "permanent": true }
  ]
}
